            <li><a href="#" onclick="goToContactpage()"><span class="material-symbols-outlined">contact_support</span> Contact Us</a></li>
        </ul>
    </div>    
    <h2>Ongoing Rides</h2>
    <ul>
        <% if (ongoingRides.length > 0) { %>
            <% ongoingRides.forEach(ride => { %>
                <li>
                    Ride from <%= ride.source %> to <%= ride.destination %> on <%= ride.date.toDateString() %>
                    <p>Status: <%= ride.status %></p>
                </li>
            <% }) %>
        <% } else { %>
            <p>No ongoing rides at the moment.</p>
        <% } %>
    </ul>

    <h2>Past Rides</h2>
    <ul>
        <% if (pastRides.length > 0) { %>
            <% pastRides.forEach(ride => { %>
                <li>
                    Ride from <%= ride.source %> to <%= ride.destination %> on <%= ride.date.toDateString() %>
                    <p>Status: <%= ride.status %></p>
                </li>
            <% }) %>
        <% } else { %>
            <p>No past rides found.</p>
        <% } %>
    </ul>

    <button onclick="window.location.href='/profile';">Back to Profile</button>
